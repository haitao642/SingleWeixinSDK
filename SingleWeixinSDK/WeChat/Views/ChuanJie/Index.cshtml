    <script src="/Scripts/Dialog.js" type="text/javascript"></script>
    <link rel="stylesheet" href="/Content/weui.css" />
</head>
<body>
    @if (!string.IsNullOrEmpty(Model.message))
    {
        <script type="text/javascript">
            Dialog("温馨提醒", '@Model.message', 1);
        </script>
    }
    <div class="am-g">
        <div class="am-u-sm-12">
            @*<div id="shuffling" class="carousel slide" data-ride="carousel" data-interval="3000">
                    <div class="carousel-inner">
                        @foreach (Model.StoreImgM m in Model.listimg)
                        {
                            if (m.order == 1)
                            {
                                <div class="item active">
                                    <img src="@m.str_DLPath" title="" class="img-responsive">
                                </div>
                            }
                            else
                            {
                                <div class="item">
                                    <img src="@m.str_DLPath" title="" class="img-responsive">
                                </div>
                            }
                        }
                    </div>

                </div>*@
            <!-- Slider 主容器，必需 -->
            <div class="swiper-container" id="swipercontainer">

                <!-- slides 容器，必需 -->
                <div class="swiper-wrapper">
                    <!-- Slides -->
                    @foreach (Model.StoreImgM m in Model.listimg)
                    {
                        <div class="swiper-slide"><img src="@m.str_DLPath" title="" style="height:250px;width:100%;"></div>
                    }

                    @*<div class="swiper-slide">Slide 2</div>
                        <div class="swiper-slide">Slide 3</div>
                        ...*@
                </div>

                <!-- 分页容器，可选 -->
                <div class="swiper-pagination"></div>

                <!-- 上下翻页箭头，可选 -->
                @*<div class="swiper-button-prev"></div>
                    <div class="swiper-button-next"></div>*@

                <!-- 滚动条容器，可选 -->
                @*<div class="swiper-scrollbar"></div>*@
            </div>
        </div>
    </div>
    <div class="am-g">
        <div class="am-u-sm-12" style="height:50px;">
            <div class="am-u-sm-3" style="height:50px;">
                <img src="" style="height:30px;width:30px" /><br />
